#!/bin/bash

TARGETS=`ls Murax*.qsf | grep -v precomp | sed "s/\.qsf//" | sort | uniq`

for t in $TARGETS; do make TARGET=$t $1; done

TARGETS=`ls Murax*.qsf | grep precomp | sed "s/\.qsf//" | sed "s/_precomp//" | sort | uniq`

for t in $TARGETS; do make TARGET=$t PRECOMP=yes $1; done

